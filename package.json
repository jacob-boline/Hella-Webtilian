{
  "dependencies": {
    "@tailwindcss/cli": "^4.0.9",
    "create-stylelint": "^0.4.0"
  },
  "name": "hella-webtilian",
  "version": "1.0.0",
  "type": "modeule",
  "main": "index.js",
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@babel/preset-env": "^7.26.7",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "concurrently": "^9.2.1",
    "css-loader": "^7.1.2",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.0.9",
    "webpack": "^5.97.1",
    "webpack-bundle-tracker": "^3.1.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "scripts": {
    "tw:dev": "tailwindcss -i ./hr_site/static/hr_site/index.css -o ./hr_site/static/hr_site/build.css --watch",
    "tw:build": "tailwindcss -i ./hr_site/static/hr_site/index.css -o ./hr_site/static/hr_site/build.css --minify",
    "django:run": ".venv/bin/python manage.py runserver 0.0.0.0:8000",
    "django:lr": ".venv/bin/python manage.py livereload",
    "dev:all": "concurrently -k -n TW,DJANGO,LR -c blue,green,magenta \"npm:tw:dev\" \"npm:django:run\" \"npm:django:lr\"",
    "wp:serve": "webpack serve --mode development",
    "wp:build": "webpack --mode production",
    "dev": "npm run tw:dev",
    "start": "concurrently \"npm:tw:dev\" \"npm:wp:serve\"",
    "build": "npm run wp:build && npm run tw:build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yc5424tl/hella-webtilian.git"
  },
